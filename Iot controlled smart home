 #include <SoftwareSerial.h> SoftwareSerial ESP(2,3); String mySSID = "SSID";     String myPWD = "PASSWORD";String sendData = "GET https://api.thingspeak.com/channels/1041331/fields/1.json?api_key=XXXXXXXXXXX&results=2";String output = ""; #define Relay1 6void setup() {  pinMode(Relay1,OUTPUT);  digitalWrite(Relay1, HIGH);  delay(2000);  Serial.begin (9600);  delay(100);  ESP.begin(9600);        ESP_talk("AT",1000);    ESP_talk("AT+RST",1000);    ESP_talk("AT+CWMODE=1",5000);    ESP_talk("AT+CWJAP=\""+ mySSID +"\",\""+ myPWD +"\"", 1000);     Serial.println("Wifi Connected");     delay(1000);  }void loop() {  String cmd = "AT+CIPSTART=\"TCP\",\"";    cmd += "184.106.153.149";    cmd += "\",80";   ESP_talk(cmd,1000);    delay(100);     ESP_talk("AT+CIPSEND=76",1000);      delay(100);  output = "";    ESP_talk(sendData,1000);    delay(100);char incoming_value=output[126];    Serial.print("incoming_value is : ");       Serial.println(incoming_value);    if (incoming_value == '0')     {            digitalWrite(Relay1,LOW);    }    else (incoming_value == '1') ;    {            digitalWrite(Relay1,HIGH);    }    }void ESP_talk(String AT_cmd, const int timeout){    Serial.print("Sent: ");  Serial.print(AT_cmd);  ESP.println(AT_cmd);   Serial.println("");      long int time = millis();  output=""; //clear the string    while ( (time + timeout) > millis())  {    while (ESP.available())    {      char i = ESP.read();       output += i;     }  }    Serial.println("Received: ");  Serial.print(output);  }